generator client {
  provider = "prisma-client-js"
}

datasource db {  
  provider = "sqlite"  
  url      = "file:./dev.db"  
}


// Mod√®le pour l'utilisateur
model User {
  id               String   @id @default(cuid())
  email            String   @unique

  isActive         Boolean  @default(false)
  subscriptionID   String?
  stripeCustomerId String?  @unique
}
